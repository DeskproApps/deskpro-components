{
  "name": "deskpro-styles",
  "version": "0.2.0",
  "description": "DeskPRO Styles",
  "private": true,
  "scripts": {
    "dev": "npm run clean && npm run styleguide && webpack-dev-server",
    "clean": "rimraf dist",
    "compile": "babel --loose es6.modules -d lib/ src/",
    "styleguide": "kss --config kss-config.json",
    "prod": "npm run clean && webpack --config webpack.prod.config.js --progress --profile --colors && npm run styleguide",
    "lint:js": "eslint . --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "test:visual": "start-storybook -p 9002 -c .storybook",
    "test:visual:build": "build-storybook -o build/storybook",
    "webpack": "webpack"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@storybook/addon-actions": "^3.0.1",
    "@storybook/addon-info": "^3.0.0",
    "@storybook/addon-knobs": "^3.0.0",
    "@storybook/addon-links": "^3.0.0",
    "@storybook/addon-notes": "^3.0.0",
    "@storybook/react": "3.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.2",
    "babel-jest": "12.1.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-namespace": "^0.2.2",
    "babel-plugin-react-intl": "^2.1.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "4.0.1",
    "cpx": "^1.5.0",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-babel-namespace": "^0.0.8",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "kss": "^3.0.0-beta.18",
    "node-sass": "4.5.2",
    "postcss-loader": "2.0.5",
    "react-addons-test-utils": "15.3.2",
    "react-storybook-addon-props-combinations": "^0.4.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.4",
    "style-loader": "0.17.0",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "bourbon": "^4.3.4",
    "bourbon-neat": "^2.0.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.5.10",
    "react": "15.3.2",
    "react-bootstrap-multiselect": "git+https://github.com/jducro/react-bootstrap-multiselect.git#deskpro",
    "react-dom": "15.3.2",
    "react-onclickoutside": "5.8.3",
    "react-select": "^1.0.0-rc.5",
    "react-svg-inline": "^2.0.0",
    "react-tagsinput": "^3.17.0"
  }
